dling\r\n                             error={ !password && submitted && error}   \r\n                             helperText={!password && submitted && error ? \"*Empty Field / Wrong Password*\" : \"\"}       \r\n                        />\r\n\r\n                    <Button \r\n                        className={classes.margin}\r\n                        color=\"primary\" \r\n                        variant=\"contained\"\r\n                        type=\"submit\" \r\n                        onClick={this.handleSubmit.bind(this)} \r\n                        style={{\r\n                            fontSize: \"1.3rem\",\r\n                            width: \"100%\",\r\n                        }}\r\n                    >\r\n                        Login\r\n                    </Button>\r\n                    <Button \r\n                        className={classes.margin}\r\n                        color=\"secondary\"\r\n                        variant=\"contained\"\r\n                        onClick={signInWithGoogle}  \r\n                        style={{\r\n                            fontSize: \"1.3rem\",\r\n                            width: \"100%\",\r\n                        }}\r\n                    >\r\n                        Continue with Google\r\n                    </Button>\r\n                </form>  \r\n                <Link to=\"/forgotpassword\" className=\"forgot-password\">\r\n                    Forgot Password\r\n                </Link>\r\n            </div>\r\n            )\r\n    }\r\n}\r\n\r\nexport default withStyles(styles)(Login)","C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\components\\RegistrationForm\\index.js",["89","90","91","92","93","94","95"],"import React, { Fragment, Component } from 'react'\r\nimport { TextField } from '@material-ui/core'\r\nimport { withStyles } from '@material-ui/styles'\r\nimport { Button } from '@material-ui/core'\r\n// import Button from '../Forms/Button'\r\nimport { auth,