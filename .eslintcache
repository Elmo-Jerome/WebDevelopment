arget.value)\r\n    }\r\n\r\n    const handleSubmit = async(e) => {\r\n        e.preventDefault()       \r\n       try {\r\n            await auth.sendPasswordResetEmail(email)\r\n            .then(() => console.log(\"send email\"))\r\n            .catch (() => console.log(\"something went wrong\"))\r\n            .then(()=>history.goBack())\r\n       } catch (err) {\r\n           console.log(err)\r\n       }\r\n    }\r\n\r\n    useEffect(()=>{\r\n        emailValidation()\r\n    },[submitted,email])\r\n\r\n    return (\r\n        <Fragment>\r\n            <div className=\"form-box\">\r\n                <div className=\"forgot-password-text\">Forgot Password</div> \r\n                <form onSubmit={handleSubmit}>\r\n                <TextField \r\n                        className={classes.margin}\r\n                        label=\"Email\"\r\n                        id=\"email\"\r\n                        type=\"email\"\r\n                        fullWidth\r\n                        value={email}\r\n                        \r\n\r\n                        // Customizations\r\n                        inputProps={{ style: { fontSize: \"1.6rem\" }}}\r\n                        InputLabelProps={{ style: { fontSize: \"1.6rem\" }}}\r\n                        FormHelperTextProps ={{ style: { fontSize: \"1.1rem\" } }}\r\n                        onChange={changeHandler}\r\n                        \r\n                        // Error Handling\r\n                        error={!validEmail && submitted}   \r\n                        helperText={!validEmail && submitted ? \"*Invalid Email*\" : \"\"}                                             \r\n                    />\r\n                    <Button \r\n                        variant=\"contained\"\r\n                        color=\"primary\"\r\n                        className={classes.margin}\r\n                        type=\"submit\"\r\n                        onClick={()=>setSubmitted(true)}\r\n                        style={{\r\n                            fontSize: \"1.3rem\",\r\n                            width: \"100%\",\r\n                        }}\r\n                    >\r\n                        Check Email\r\n                    </Button>\r\n                </form>\r\n            </div>\r\n        </Fragment>\r\n    )\r\n}\r\n\r\nexport default ForgotPassForm\r\n\r\n","C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\components\\RegistrationForm\\alt.js",[],"C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\redux\\Reducers\\index.js",[],"C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\redux\\Reducers\\userReducer.js",[],"C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\redux\\Types\\index.js",[],"C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\redux\\Actions\\index.js",[],"C:\\Users\\cachu\\Desktop\\WebDev\\React\\miniProjects\\ecommerce\\oshoppee\\src\\redux\\store.js",[],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":38,"column":25,"nodeType":"128","endLine":38,"endColumn":59},{"ruleId":"129","severity":1,"message":"130","line":26,"column":8,"nodeType":"131","endLine":26,"endColumn":32,"suggestions":"132"},{"ruleId":"122","replacedBy":"133"},{"ruleId":"124","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":61,"column":44,"nodeType":"137","messageId":"138","endLine":61,"endColumn":45,"suggestions":"139"},{"ruleId":"135","severity":1,"message":"136","line":61,"column":64,"nodeType":"137","messageId":"138","endLine":61,"endColumn":65,"suggestions":"140"},{"ruleId":"141","severity":1,"message":"142","line":145,"column":17,"nodeType":"143","messageId":"144","endLine":145,"endColumn":23},{"ruleId":"141","severity":1,"message":"145","line":145,"column":25,"nodeType":"143","messageId":"144","endLine":145,"endColumn":30},